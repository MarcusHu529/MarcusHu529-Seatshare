<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:type="radial"
                android:centerColor="#E0FFFFFF"
                android:endColor="#B0FFFFFF"
                android:gradientRadius="100dp" />
            <corners android:radius="20dp" />
            <stroke
                android:width="2dp"
                android:color="#18453B" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:type="linear"
                android:startColor="#F0FFFFFF"
                android:endColor="#D0FFFFFF"
                android:angle="135" />
            <corners android:radius="20dp" />
            <stroke
                android:width="1dp"
                android:color="#18453B" />
        </shape>
    </item>
</selector>